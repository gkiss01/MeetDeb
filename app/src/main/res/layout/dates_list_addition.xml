<?xml version="1.0" encoding="utf-8"?>
<com.skydoves.expandablelayout.ExpandableLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/expandableLayout"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    app:expandable_duration="300"
    app:expandable_parentLayout="@layout/dates_list_addition_collapsed"
    app:expandable_secondLayout="@layout/dates_list_addition_expanded"
    app:expandable_showSpinner="true"
    app:expandable_spinner="@drawable/ic_arrow_down"
    app:expandable_spinner_animate="true"
    app:expandable_spinner_margin="14dp"
    app:expandable_spinner_size="32dp" />